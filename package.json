{
  "name": "x6-hooks",
  "version": "1.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "antv",
    "x6",
    "graph",
    "svg",
    "vue",
    "react",
    "hooks",
    "typescript"
  ],
  "module": "dist/react.js",
  "author": "lloydzhou",
  "license": "MIT",
  "homepage": "https://github.com/lloydzhou/x6-hooks",
  "repository": {
    "type": "git",
    "url": "git@github.com:lloydzhou/x6-hooks.git"
  },
  "dependencies": {},
  "scripts": {
    "update": "git submodule update",
    "install:react": "cd react && yarn install",
    "install:vue": "cd vue && yarn install",
    "install:dev": "run-p install:react install:vue",
    "build:react": "cd react && yarn run build",
    "build:vue": "cd vue && yarn run build",
    "build:source": "run-p build:react build:vue",
    "build:clean": "rm -rf dist && mkdir -p dist",
    "build:copy:utils": "cp react/dist/utils.js dist/utils.js && cp react/dist/utils.d.ts dist",
    "build:copy:react": "cp react/dist/hooks.d.ts dist/react.d.ts && cp react/dist/hooks.js dist/react.js",
    "build:copy:vue": "cp vue/dist/hooks.js dist/vue.js && cp vue/dist/hooks.d.ts dist/vue.d.ts",
    "build:copy": "run-p build:copy:utils build:copy:react build:copy:vue",
    "build": "run-s build:source build:clean build:copy"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
